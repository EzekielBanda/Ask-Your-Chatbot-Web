{
  "redirects": [
    {
      "source": "/",
      "destination": "/nbs-legal-chatbot/",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/nbs-legal-chatbot/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/nbs-legal-chatbot/service-worker.js",
      "destination": "/service-worker.js"
    },
    {
      "source": "/nbs-legal-chatbot/manifest.json",
      "destination": "/manifest.json"
    },
    {
      "source": "/nbs-legal-chatbot/(.*)\\.(js|css|png|svg|ico|json|webmanifest)",
      "destination": "/$1"
    },
    {
      "source": "/nbs-legal-chatbot/(.*)",
      "destination": "/index.html"
    }
  ]
}
