{
  "redirects": [
    {
      "source": "/",
      "destination": "/nbs-legal-chatbot/",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/nbs-legal-chatbot/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/nbs-legal-chatbot/manifest.json",
      "destination": "/manifest.json"
    },
    {
      "source": "/nbs-legal-chatbot/(.*)",
      "destination": "/index.html"
    }
  ]
}
